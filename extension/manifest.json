{
    "manifest_version": 2,
    "name": "Just another speed dial",
    "version": "0.0.7",
    "description": "Shows a speed dial made of bookmarks",
    "chrome_url_overrides": {
        "newtab": "dial.html"
    },
    "permissions": [
        "bookmarks",
        "storage"
    ],
    "applications": {
        "gecko": {
            "id": "justAnotherSpeedDial@gmail.com",
            "strict_min_version": "57.0"
        }
    },
    "background": {
        "page": "background/background.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
}