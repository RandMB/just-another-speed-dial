{
    "manifest_version": 2,
    "name": "Just another speed dial",
    "version": "0.0.3",
    "description": "Shows a speed dial made of bookmarks",
    "chrome_url_overrides": {
        "newtab": "speed-dial-app/index.html"
    },
    "permissions": [
        "bookmarks",
        "storage",
        "*://s2.googleusercontent.com/s2/favicons"
    ],
    "applications": {
        "gecko": {
            "id": "justAnotherSpeedDial@gmail.com",
            "strict_min_version": "57.0"
        }
    },
    "background": {
        "page": "background/background.html"
    },
    "options_ui": {
        "page": "options-app/index.html",
        "open_in_tab": true
    }
}